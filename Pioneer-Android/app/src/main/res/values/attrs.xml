<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AppTitleBar">
        <attr name="titleText" format="string" />
        <attr name="backRes" format="reference" />
        <attr name="menuRes" format="reference" />

        <attr name="titleEnable" format="enum">
            <enum name="visible" value="0x00000000" />
            <enum name="invisible" value="0x00000004" />
            <enum name="gone" value="0x00000008" />
        </attr>

        <attr name="backEnable" format="enum">
            <enum name="visible" value="0x00000000" />
            <enum name="invisible" value="0x00000004" />
            <enum name="gone" value="0x00000008" />
        </attr>

        <attr name="menuEnable" format="enum">
            <enum name="visible" value="0x00000000" />
            <enum name="invisible" value="0x00000004" />
            <enum name="gone" value="0x00000008" />
        </attr>

    </declare-styleable>

    <declare-styleable name="LineButtonView">
        <attr name="iconSize" format="dimension" />
        <attr name="itemTextSize" format="dimension" />
        <attr name="itemTextMargin" format="dimension" />
        <attr name="itemTextColor" format="color" />
        <attr name="itemText" format="string" />
        <attr name="leftDrawable" format="reference" />
        <attr name="rightDrawable" format="reference" />
    </declare-styleable>

</resources>